{
  "name": "@oobereats/auth-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "prestart": "npm run migration:run",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "tsc -p tsconfig.build.json",
    "prebuild": "npm run clean",
    "build:prod": "npm run build",
    "clean": "rimraf dist/src/",
    "test": "npm run test:migration && npm run test:ci && npm run test:e2e",
    "test:unit": "jest",
    "test:migration": "dotenv -e env.test --  typeorm migration:run -t each",
    "test:ci": "NODE_ENV=CI jest --ci none --testPathPattern='src' --forceExit true --collectCoverage --maxWorkers 2 --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest.config-e2e.js --runInBand --silent --detectOpenHandles",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:generate": "typeorm migration:create -n -d ./dist/data-source.js",
    "migration:run": "typeorm migration:run -t each -d ./dist/data-source.js",
    "migration:revert": "typeorm migration:revert -d ./dist/data-source.js",
    "db:sync": "npm run typeorm:cli schema:sync"
  },
  "dependencies": {
    "@nanogiants/nestjs-swagger-api-exception-decorator": "^1.6.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "parse-domain": "^7.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.14",
    "url-join-ts": "^1.0.5",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^8.0.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "mock-jwks": "^2.0.3",
    "nock": "^13.3.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.3.2",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  }
}
